{
    "name": "eroluysal/settings",
    "description": "Database Settings Package for Laravel Framework.",
    "authors": [
        {
            "name": "Erol Uysal",
            "email": "eutkuuysal@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "QweB\\Settings\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "require": {
        "php": ">=5.4.0",
        "illuminate/cache": "~5.0",
        "illuminate/database": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "mockery/mockery": "~1.0"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
